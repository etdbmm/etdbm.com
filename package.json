{
  "name": "eti",
  "author": "Mars Collective <hi@marscollective.co>",
  "version": "0.0.1",
  "license": "0BSD",
  "private": true,
  "scripts": {
    "dev": "gatsby develop",
    "build": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "stage": "yarn build && netlify deploy",
    "deploy": "yarn stage -p",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,mdx,yaml,yml}\""
  },
  "dependencies": {
    "@marscollective/gatsby-theme-core": "^0.2.6",
    "autoprefixer": "^10.1.0",
    "gatsby": "^2.29.2",
    "gatsby-background-image": "^1.3.1",
    "gatsby-plugin-postcss": "^3.4.0",
    "postcss": "^8.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-icons": "^4.1.0",
    "react-netlify-forms": "^1.2.0",
    "tailwindcss": "^2.0.2",
    "typeface-quicksand": "^1.1.13"
  },
  "devDependencies": {
    "prettier": "2.2.1"
  },
  "prettier": {
    "trailingComma": "none",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "arrowParens": "avoid"
  }
}
